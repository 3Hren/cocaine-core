#!/usr/bin/scons

import os

Import(['env'])

plugin = env.SharedLibrary(
    target = "#build/plugins/mysql",
    SHLIBPREFIX = "",
    source = ["mysql.cpp"],
    CPPPATH = ["#include", "/usr/include/mysql"],
    LIBS = ["uriparser", "mysqlclient"],
    LINKFLAGS = ["-Wl,-Bsymbolic"])

Return("plugin")
