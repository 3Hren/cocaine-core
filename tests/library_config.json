{
    "version": 1,
    "paths": {
        "plugins": "root/lib/cocaine",
        "ipc": "root/run",
        "spool": "root/spool"
    },
    "storages": {
        "core": {
            "type": "elliptics",
            "args": {
                "nodes": {
                    "elisto20f.dev.yandex.net": 1025
                },
                "groups": [1, 2],
                "verbosity": 10
            }
        },
        "core:cache": {
            "type": "files",
            "args": {
                "path": "root/cache"
            }
        }
    }
}
